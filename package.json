{
  "name": "cursor-orchestrator",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm run dev -w packages/web",
    "dev:server": "npm run dev -w packages/server",
    "build:web": "npm run build -w packages/web",
    "build:server": "npm run build -w packages/server",
    "lint:web": "npm run lint -w packages/web",
    "lint:server": "npm run lint -w packages/server",
    "test:web": "npm test -w packages/web",
    "test:server": "npm test -w packages/server",
    "dev": "npm run dev:server",
    "build": "npm run build:server && npm run build:web"
  }
}
